{
  "name": "astrogent",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "node scripts/copy-robots.mjs && astro build",
    "preview": "astro preview",
    "format": "prettier --write .",
    "astro": "astro"
  },
  "dependencies": {
    "@iconify-json/simple-icons": "^1.2.69",
    "@lucide/astro": "^0.563.0",
    "astro": "^5.17.1",
    "astro-icon": "^1.1.5",
    "marked": "^15.0.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.16"
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-astro",
      "prettier-plugin-tailwindcss"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
